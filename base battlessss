Tab:AddButton({
	Name = "X2 Drops (24HRS)",
	Callback = function()
        while true do
            local args = {
                [1] = "x2 Drops (24 Hours)",
                [2] = false
            }
            
            game:GetService("ReplicatedStorage").Remotes.ToServer.PremRedeem:InvokeServer(unpack(args))
            wait(0.5)
            end
  	end    
})

